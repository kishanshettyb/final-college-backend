{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "singularName": "result",
    "pluralName": "results",
    "displayName": "result"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
