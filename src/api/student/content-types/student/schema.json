{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "male",
        "female"
      ]
    },
    "usn": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "age": {
      "type": "integer"
    },
    "branch": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::branch.branch"
    },
    "result": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::result.result",
      "inversedBy": "students"
    }
  }
}
